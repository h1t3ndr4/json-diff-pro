{
  "name": "json-diff-pro",
  "displayName": "JSON Diff Pro",
  "description": "Professional JSON comparison tool with native implementation",
  "version": "1.0.0",
  "publisher": "h1t3ndr4",
  "author": {
    "name": "Hitendra Mali",
    "email": "hitumali10@gmail.com"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Formatters",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:json-diff-pro.compare"
  ],
  "main": "./index.js",
  "contributes": {
    "commands": [
      {
        "command": "json-diff-pro.compare",
        "title": "Compare JSON",
        "category": "JSON Diff Pro"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == json",
          "command": "json-diff-pro.compare",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "eslint": "^8.47.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h1t3ndr4/json-diff-pro.git"
  },
  "license": "MIT"
}
